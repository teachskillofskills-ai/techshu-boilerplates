  "api": {
    "baseUrl": "https://raw.githubusercontent.com/teachskillofskills-ai/techshu-boilerplates/main",
    "endpoints": {
      "registry": "/registry.json",
      "boilerplate": "/{boilerplate-id}",
      "component": "/{boilerplate-id}/components/{component-name}",
      "lib": "/{boilerplate-id}/lib/{lib-name}",
      "readme": "/{boilerplate-id}/README.md"
    }
  },
  "cli": {
    "install": "npm install -g @techshu/cli",
    "usage": "techshu add <boilerplate-id>",
    "commands": {
      "list": "techshu list",
      "search": "techshu search <query>",
      "add": "techshu add <boilerplate-id>",
      "info": "techshu info <boilerplate-id>",
      "update": "techshu update"
    }
  }
